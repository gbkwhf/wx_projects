/*---------------搜索框----------  */
::-webkit-scrollbar {
			display: none;
		}
.searchTop {
  height: 130rpx;
  width: 100%;
  /*top: 0;
	bottom: 80px;*//*background-color: #ffffff;*//*position: absolute;*/
  z-index: 1000;
  position: fixed;
}

.active {
  background-color: #fff;
}

.searchBox {
  width: 79%;
  height: 60rpx;
  margin-left: 38rpx;
  margin-top: 32rpx;
  border-radius: 40rpx;
  /*border: solid 1px #bfbfbf;*/
  float: left;
  margin-bottom: 28rpx;
  background: rgba(0, 0, 0, 0.1);
}

.seachImg {
  float: left;
}

.seachImg image {
  width: 26rpx;
  height: 26rpx;
  margin-left: 30rpx;
  margin-top: 16rpx;
}

.insearchBox {
  float: left;
  width: 85%;
  margin-left: 10rpx;
}

.insearchBox input {
  background-color: transparent;
}

.insearch {
  height: 60rpx;
  width: 90%;
  line-height: 60rpx;
  outline: none;
  border: none;
  padding-left: 5px;
  font-size: 28rpx;
}

.searchSub {
  font-size: 28rpx;
  color: #fff;
  line-height: 60rpx;
  float: right;
  padding-top: 32rpx;
  padding-right: 30rpx;
}

.insearchs {
  color: red;
}

/*-----------轮播图-------  */

.swiper image {
  width: 100%;
  height: auto;
}

.swiper-image {
  height: 100%;
  width: 100%;
}

.slide-image {
  height: 100%;
  width: 100%;
  display: block;
}

/* --------商品分类--------- */

.container {
  clear: both;
  padding: 2rpx 24rpx;
}

.classificationBox {
  overflow: hidden;
  width: 100%;
}

.classification {
  float: left;
  width: 20%;
  text-align: center;
  margin-top: 42rpx;
}

.classification image {
  width: 92rpx;
  height: 92rpx;
  margin: 0 auto;
  vertical-align: top;
}

.classification text {
  font-style: normal;
  font-size: 24rpx;
  color: #4d4d4d;
  display: block;
  width: 100%;
  text-align: center;
  margin-top: 18rpx;
}

/* -----------分割线---------- */

.cut_off {
  height: 12rpx;
  background-color: #f3f2f2;
  margin-top: 20rpx;
}

/* ---------福利区---------- */

.gift_box {
  width: 92%;
  margin: 0 auto;
  margin-top: 30rpx;
}

.gift_box image {
  height: 170rpx;
  width: 100%;
}

/* ----------新品上线-------- */

.new_shop {
  margin-top: 40rpx;
  height: 54rpx;
}

.new_box {
  float: left;
  width: 198rpx;
  height: 54rpx;
  text-align: center;
  line-height: 54rpx;
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  margin-left: 276rpx;
  /* margin-top: 23px; */
  color: #333;
  font-size: 28rpx;
}

/*--------商品信息-----*/

.shop_Box {
  min-height: 200rpx;
  margin-bottom: 120rpx;
  background: #f5f5f5;
  overflow: auto;
  margin-top: 34rpx;
}

.shopBox {
  /*clear: both;*//*margin-top: 15px;*/
  width: 46%;
  /* height: 276px; *//*border-right: solid 2px #e5e5e5;
    border-top: solid 2px #e5e5e5;*/
  overflow: auto;
  float: left;
  background-color: #fff;
  margin: 0rpx 10rpx 20rpx 20rpx;
  /*margin-bottom: 15px;*/
}

.shopBox:nth-child(even) {
  border-right: none;
  width: 46%;
  margin: 0rpx 10rpx 20rpx 10rpx;
}

.shopBox:nth-child(1) {
  margin-top: 20rpx;
}

.shopBox:nth-child(2) {
  margin-top: 20rpx;
}

.shopImg {
  width: 100%;
  height: 230rpx;
  margin: 0 auto;
}

.shopImg image {
  width: 100%;
  height: 230rpx;
  margin: 0 auto;
  /* margin-top: 15px;*/
}

.shopName {
  height: 40rpx;
  padding: 30rpx 40rpx 40rpx 24rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  /* white-space: nowrap; */
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  text-align: left;
}

.shopMessage {
  height: 54rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.shops {
  float: left;
}

.shopPrice {
  color: #ec4444;
  padding-left: 24rpx;
  font-size: 28rpx;
}

.fan {
  color: #acacac;
  font-size: 24rpx;
  padding-left: 22rpx;
}

.markPrice {
  text-decoration-line: line-through;
  color: #acacac;
  font-size: 28rpx;
  padding-left: 20rpx;
}
